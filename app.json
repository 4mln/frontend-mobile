{
  "expo": {
    "name": "B2B Marketplace",
    "slug": "b2b-marketplace",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.b2bmarketplace.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/android-icon-foreground.png",
        "backgroundImage": "./src/assets/images/android-icon-background.png",
        "monochromeImage": "./src/assets/images/android-icon-monochrome.png"
      },
      "package": "com.b2bmarketplace.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png",
      "bundler": "metro",
      "output": "static",
      "name": "B2B Marketplace",
      "shortName": "B2B Market",
      "description": "Professional B2B marketplace for business-to-business transactions",
      "themeColor": "#f97316",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "scope": "/",
      "lang": "en",
      "dir": "ltr"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Vazir-Regular.ttf",
            "./src/assets/fonts/Vazir-Medium.ttf",
            "./src/assets/fonts/Vazir-Bold.ttf"
          ]
        }
      ],
      [
        "expo-localization",
        {
          "supportsRTL": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
