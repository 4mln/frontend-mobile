{
  "name": "b2b-marketplace",
  "main": "index.js",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:e2e": "detox test",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:web": "expo export -p web",
    "deploy:web": "bash scripts/deploy-web.sh",
    "deploy:mobile": "bash scripts/deploy-mobile.sh",
    "deploy:all": "npm run deploy:web && npm run deploy:mobile",
    "backend:start": "cd c:\\b2b-marketplace && docker-compose up -d",
    "backend:stop": "cd c:\\b2b-marketplace && docker-compose down",
    "backend:logs": "cd c:\\b2b-marketplace && docker-compose logs -f api",
    "backend:test": "node scripts/test-backend-connection.js",
    "dev:full": "npm run backend:start && npm start"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.7",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0",
    "expo-constants": "~18.0.8",
    "expo-crypto": "~15.0.7",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-router": "3.5.23",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "i18next": "^23.0.0",
    "nativewind": "^2.0.11",
    "react-hook-form": "^7.48.0",
    "react-i18next": "^13.5.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-skeleton-content": "1.0.13",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.19.12",
    "react-native-worklets": "0.5.1",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.6",
    "@types/react-native": "^0.72.8",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "expo": "54.0.7",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react-native-reanimated": "4.1.0"
  }
}